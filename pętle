//1. Za pomocą pętli while wyświetlić liczby z zakresu 10 do 35.
for (int i=10;i<=35;i++)
{
Console.WriteLine(i);  
}
Console.ReadKey();
Console.Clear();
//2. Pobiera od użytkownika liczbę całkowitą n i oblicza wartość n!
Console.Write("Podaj liczbe calkowita: ");
int liczba = Convert.ToInt32(Console.ReadLine());
int suma = 1;
if (liczba == 0)
{
 Console.WriteLine("Silnia jest rowna 1");
}
else
{
for (int i = 1; i <= liczba; i++)
 {
suma = suma * i;
Console.WriteLine(suma);
}
}
Console.ReadKey();
Console.Clear();
//3.drukuje na ekranie kwadrat. Długość boku kwadratu wczytujemy z klawiatury. Poniższy kwadrat ma bok długości 4. 
Console.WriteLine("Podaj Dlugosc Boku Kwadratu");
int dlugosc = Convert.ToInt32(Console.ReadLine());
int druga = dlugosc;
for (int i = 1; i <= druga; i++)
{
    for (int a = 1; a <= dlugosc; a++)
    {
        Console.Write("K");
    }
    Console.WriteLine();
}
Console.ReadKey();
